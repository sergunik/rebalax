            "targets": [
                {
                    "datasource": {
                        "type": "grafana-postgresql-datasource",
                        "uid": "rebalax_postgres_db"
                    },
                    "editorMode": "code",
                    "format": "table",
                    "rawQuery": true,
                    "rawSql": "WITH paired_tokens AS (\n  SELECT\n    t1.token_symbol AS token_x,\n    t2.token_symbol AS token_y,\n    1 AS paired\n  FROM\n    public.portfolio_assets AS t1\n    JOIN public.portfolio_assets AS t2 ON t1.portfolio_id = t2.portfolio_id\n    AND t1.id < t2.id\n  WHERE\n    t1.portfolio_id IN (\n      SELECT portfolio_id\n      FROM public.portfolio_assets\n      GROUP BY portfolio_id\n      HAVING COUNT(*) = 2\n    )\n),\nall_tokens AS (\n  SELECT DISTINCT token_symbol FROM public.portfolio_assets\n),\nall_pairs AS (\n  SELECT \n    a.token_symbol AS token_x,\n    b.token_symbol AS token_y,\n    0 AS paired\n  FROM all_tokens a\n  CROSS JOIN all_tokens b\n  WHERE a.token_symbol < b.token_symbol\n)\nSELECT \n  token_x,\n  token_y,\n  MAX(paired) AS paired\nFROM (\n  SELECT * FROM paired_tokens\n  UNION ALL\n  SELECT * FROM all_pairs\n) combined\nGROUP BY token_x, token_y\n",
                    "refId": "A",
                    "sql": {
                        "columns": [
                            {
                                "parameters": [],
                                "type": "function"
                            }
                        ],
                        "groupBy": [
                            {
                                "property": {
                                    "type": "string"
                                },
                                "type": "groupBy"
                            }
                        ],
                        "limit": 50
                    }
                }
            ],
            "title": "Pairs",
