                {
                    "datasource": {
                        "type": "grafana-postgresql-datasource",
                        "uid": "rebalax_postgres_db"
                    },
                    "editorMode": "code",
                    "format": "table",
                    "rawQuery": true,
                    "rawSql": "SELECT \n    pa.portfolio_id,\n    pa.token_symbol,\n    pa.quantity,\n    rl.price_usd,\n    (pa.quantity * rl.price_usd) AS total_value_usd\nFROM portfolio_assets pa\nJOIN rebalance_logs rl \n  ON pa.portfolio_id = rl.portfolio_id\n  AND pa.token_symbol = rl.token_symbol\nWHERE rl.executed_at = (\n    SELECT MAX(executed_at)\n    FROM rebalance_logs r2\n    WHERE r2.portfolio_id = rl.portfolio_id\n      AND r2.token_symbol = rl.token_symbol\n)\nORDER BY total_value_usd DESC\nLIMIT 10;",
                    "refId": "A",
                    "sql": {
                        "columns": [
                            {
                                "parameters": [],
                                "type": "function"
                            }
                        ],
                        "groupBy": [
                            {
                                "property": {
                                    "type": "string"
                                },
                                "type": "groupBy"
                            }
                        ],
                        "limit": 50
                    },
                    "table": "rebalance_logs"
                }
